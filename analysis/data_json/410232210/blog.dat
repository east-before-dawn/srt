7684\u6a21\u578b</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;node&nbsp;=&nbsp;osgDB::readNodeFile(<span style=\"color: rgb(128,0,0);\">\"</span><span style=\"color: rgb(128,0,0);\">glider.osg</span><span style=\"color: rgb(128,0,0);\">\"</span>);<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,128,0);\">//</span><span style=\"color: rgb(0,128,0);\">\u6dfb\u52a0\u5230\u573a\u666f</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;root-&gt;addChild(node);<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,128,0);\">//</span><span style=\"color: rgb(0,128,0);\">\u4f18\u5316\u573a\u666f\u6570\u636e</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;osgUtil::Optimizer&nbsp;optimizer&nbsp;;<br>&nbsp;&nbsp;&nbsp;&nbsp;optimizer.optimize(root)&nbsp;;<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,128,0);\">//</span><span style=\"color: rgb(0,128,0);\">\u8bbe\u7f6e\u573a\u666f\u6570\u636e</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;viewer-&gt;setSceneData(root);<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,128,0);\">//</span><span style=\"color: rgb(0,128,0);\">\u521d\u59cb\u5316\u5e76\u521b\u5efa\u7a97\u53e3</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;viewer-&gt;realize();<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,128,0);\">//</span><span style=\"color: rgb(0,128,0);\">\u5f00\u59cb\u6e32\u67d3</span><span style=\"color: rgb(0,128,0);\">                <br>            </span>&nbsp;&nbsp;&nbsp;&nbsp;viewer-&gt;run();<br>            <br>&nbsp;&nbsp;&nbsp;&nbsp;<span style=\"color: rgb(0,0,255);\">return</span>&nbsp;<span style=\"color: rgb(128,0,128);\">0</span>&nbsp;;<br