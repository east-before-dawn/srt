<p>program exe1;<br>var a:Array[1..1000000,0..1]of longint;<br>&nbsp;&nbsp;&nbsp; i,j,k,l,m,n,left,right,left1,right1,sum:longint;<br>&nbsp;&nbsp;&nbsp; b:Array[1..1000000,0..2]of longint;<br>&nbsp;&nbsp;&nbsp; c:array[1..1000000,0..1]of longint;<br>function max(a,b:longint):longint;<br>begin<br>&nbsp; if a&gt;b then<br>&nbsp;&nbsp;&nbsp; max:=a<br>&nbsp; else<br>&nbsp;&nbsp;&nbsp; max:=b;<br>end;<br>function max1(p,x,y,l,r:longint):longint;<br>var mid:longint;<br>begin<br>&nbsp; if x&lt;=y then<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; mid:=(l+r) div 2;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (x=l)and(y=r) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max1:=a[p,0]<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (x&gt;mid)or(y&lt;=mid) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if x&gt;mid then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max1:=max1(2*p+1,x,y,mid+1,r);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if y&lt;=mid then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max1:=max1(2*p,x,y,l,mid);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max1:=max(max1(2*p,x,mid,l,mid),max1(2*p+1,mid+1,y,mid+1,r));<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp; end<br>&nbsp; else<br>&nbsp;&nbsp;&nbsp; max1:=0;<br>end;<br>function max2(p,x,y,l,r:longint):longint;<br>var mid:longint;<br>begin<br>&nbsp; if x&lt;=y then<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; mid:=(l+r) div 2;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (x=l)and(y=r) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max2:=a[p,1]<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (x&gt;mid)or(y&lt;=mid) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if x&gt;mid then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max2:=max2(2*p+1,x,y,mid+1,r);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if y&lt;=mid then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max2:=max2(2*p,x,y,l,mid);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; max2:=max(max2(2*p,x,mid,l,mid),max2(2*p+1,mid+1,y,mid+1,r));<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp; end<br>&nbsp; else<br>&nbsp;&nbsp;&nbsp; max2:=0;<br>end;<br>procedure qsort(l,r:longint);<br>var i,j:longint;<br>&nbsp;&nbsp;&nbsp; t:array[0..2]of longint;<br>begin<br>&nbsp; if l&lt;r then<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; i:=l;j:=r;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; while i&lt;j do<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; while (b[i,1]&lt;b[j,1])and(i&lt;j) do<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; i:=i+1;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; t:=b[i];b[i]:=b[j];b[j]:=t;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; while (b[i,1]&lt;b[j,1])and(i&lt;j) do<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; j:=j-1;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; t:=b[i];b[i]:=b[j];b[j]:=t;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; qsort(l,i-1);qsort(i+1,r);<br>&nbsp;&nbsp;&nbsp; end;<br>end;<br>procedure insert(p,x,l,r:longint);<br>var mid:longint;<br>begin<br>&nbsp; mid:=(l+r)div 2;<br>&nbsp; if (l=r)and(l=x) then<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a[p,1]:=left+1;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a[p,0]:=right+1;<br>&nbsp;&nbsp;&nbsp; end<br>&nbsp; else<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if x&gt;mid then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; insert(2*p+1,x,mid+1,r)<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; insert(2*p,x,l,mid);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a[p,0]:=max(a[2*p,0],a[2*p+1,0]);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a[p,1]:=max(a[2*p,1],a[2*p+1,1]);<br>&nbsp;&nbsp;&nbsp; end;<br>end;<br>begin<br>&nbsp; readln(m);<br>&nbsp; for i:=1 to m do<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; readln(b[i,0],b[i,1]);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; b[i,2]:=i;<br>&nbsp;&nbsp;&nbsp; end;<br>&nbsp; qsort(1,m);<br>&nbsp; fillchar(a,sizeof(a),0);<br>&nbsp; for i:=1 to m do<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; left:=max1(1,1,b[i,0]-1,1,99999);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; right:=max2(1,b[i,0]+1,99999,1,99999);<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; c[i,1]:=left+1;c[i,0]:=right+1;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; insert(1,b[i,0],1,99999);<br>&nbsp;&nbsp;&nbsp; end;<br>&nbsp; for i:=1 to m do<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if c[i,0]&gt;sum then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; sum:=c[i,0];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; j:=b[i,0];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if c[i,1]&gt;sum then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; sum:=c[i,1];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; j:=b[i,0];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp; end;<br>&nbsp; i:=m;k:=-1;writeln(sum);<br>&nbsp; while sum&gt;0 do<br>&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (sum=c[i,0])and(k&lt;&gt;0) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if ((k=1)and(b[i,0]&lt;j))or(k=-1) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; write(b[i,2],' ');<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; dec(sum);k:=0;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; j:=b[i,0];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; else<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if (sum=c[i,1])and(k&lt;&gt;1) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if ((k=0)and(b[i,0]&gt;j))or(k=-1) then<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; begin<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; write(b[i,2],' ');<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; dec(sum);k:=1;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; j:=b[i,0];<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; end;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; dec(i);<br>&nbsp;&nbsp;&nbsp; end;<br>&nbsp; writeln;<br>end.<br>在自己电脑上通过了，但URAL上说编译没通过。。。。。。。。。。。。。。。</p> <!--XN_AntiSpam_Robot 2009-06-16-->
